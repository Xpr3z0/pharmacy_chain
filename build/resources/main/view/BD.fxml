<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox prefWidth="903.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <ChoiceBox fx:id="tableChoice" prefHeight="26.0" prefWidth="130.0" />
          <Button fx:id="addTable" mnemonicParsing="false" onAction="#onActionAdd" prefHeight="28.0" text="Добавить" />
          <Button id="addTable" fx:id="deleteTable" mnemonicParsing="false" onAction="#onActionDelete" prefHeight="29.0" text="Удалить" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
    <TableView fx:id="tableView" prefHeight="514.0" prefWidth="903.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
